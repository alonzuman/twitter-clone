 { CLEAR_PROFILE, SET_PROFILE, ERROR, IS_FETCHING } from '../reducers/profile';\nconst Users = db.collection('users');\n\nexport const initialState = {\n  user: {\n    id: '',\n    displayName: '',\n    username: '',\n    following: [],\n    followers: [],\n    email: '',\n    emailVerified: null,\n  },\n  isAuth: false,\n  isFetching: false,\n}\n\nexport const ProfileContext = createContext({})\n\nconst ProfileProvider = ({ children }) => {\n  const [state, dispatch] = useReducer(profileReducer);\n\n  const signOut = async () => {\n    await auth.signOut();\n    dispatch({\n      type: CLEAR_PROFILE\n    })\n  }\n\n  const setUser = async () => {\n    dispatch({\n      type: IS_FETCHING\n    })\n    let user = {};\n    const { uid, displayName, email, photoURL, phoneNumber, emailVerified, isAnonymous } = auth.currentUser\n\n    const snapshot = await Users.doc(uid).get();\n\n    if (snapshot.exists) {\n      user = {\n        id: snapshot.id,\n        ...snapshot.data()\n      }\n    } else {\n      if (isAnonymous) {\n        user = {\n          uid,\n          displayName: faker.name.findName(),\n          username: faker.name.firstName(),\n          avatar: 'https://gravatar.com/avatar/fc2e138321e1a579e919902be761c7fa?s=400&d=robohash&r=x',\n          isAnonymous,\n          bio: faker.lorem.paragraph(),\n          following: [],\n          followers: [],\n          createdAt: Date.now()\n        }\n      } else {\n        user = {\n          uid,\n          displayName,\n          username: email.split('@')[0],\n          email,\n          avatar: photoURL,\n          phoneNumber,\n          emailVerified,\n          followers: [],\n          following: [],\n          createdAt: Date.now()\n        }\n      }\n      await Users.doc(uid).set(user);\n    }\n\n    dispatch({\n      type: SET_PROFILE,\n      payload: user\n    })\n  }\n\n  useEffect(() => {\n    auth.onAuthStateChanged(user => {\n      if (user) {\n        setUser()\n      } else {\n        signOut()\n      }\n    })\n  }, [auth.currentUser])\n\n  const value = {\n    ...state,\n    signOut,\n  }\n\n  return (\n    <ProfileContext.Provider value={value}>\n      {children}\n    </ProfileContext.Provider>\n  )\n}\n\nexport default ProfileProvider\n","/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Dialog/Dialog.js",["401","402","403"],"import React, { Suspense, useCallback, useEffect, useRef } from 'react';\nimport useClickAway from '../../../hooks/useClickAway';\nimport './Dialog.css';\n\nconst Dialog = ({ open, onClose, children }) => {\n  const dialogRef = useRef(null)\n  const { clickedAway } = useClickAway(dialogRef);\n\n  useEffect(() => {\n    if (clickedAway) {\n      return onClose()\n    }\n  }, [clickedAway])\n\n  useEffect(() => {\n    if (open) {\n      document.body.style.overflow = 'hidden';\n    }\n\n    return () => document.body.style.overflow = 'auto';\n  }, [open])\n\n  const escFunction = useCallback(event => {\n    if (event.keyCode === 27) {\n      return onClose();\n    }\n  })\n\n  useEffect(() => {\n    document.addEventListener('keydown', escFunction, false);\n    return () => document.removeEventListener('keydown', escFunction, false);\n  }, [])\n\n  if (open) {\n    return (\n      <Suspense fallback={null}>\n        <div className='dialog__wrapper'>\n          <div ref={dialogRef} className='dialog__container'>\n            {children}\n          </div>\n        </div>\n      </Suspense>\n    )\n  } else {\n    return null;\n  }\n}\n\nexport default Dialog\n","/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/contexts/UsersContext.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/hooks/useClickAway.js",["404"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/TweetPage/TweetPageTweet.js",["405"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/cards/TweetCard/TweetCardAvatar.js",[],"/Users/alonzuman/code/alonz