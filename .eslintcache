    const user = {\n        id: snapshot.id,\n        ...snapshot.data()\n      };\n      dispatch({\n        type: SET_USER,\n        payload: user\n      })\n    })\n  }\n\n  const signInWithProvider = async (provider) => {\n    dispatch({\n      type: IS_FETCHING\n    })\n\n    const firebaseProvider = () => {\n      switch (provider) {\n        case 'facebook': return new firebase.auth.FacebookAuthProvider();\n        case 'google': return new firebase.auth.GoogleAuthProvider();\n        default: return null\n      }\n    }\n\n    await auth.setPersistence(firebase.auth.Auth.Persistence.LOCAL).then(async () => {\n      const { user: { uid, displayName, email, photoURL: avatar, phoneNumber, emailVerified } } = await auth.signInWithPopup(firebaseProvider());\n\n      const userSnapshot = await Users.doc(uid).get();\n      if (!userSnapshot.exists) {\n        await Users.doc(uid).set({ uid, displayName, email, photoURL: avatar, phoneNumber, emailVerified }, { merge: true })\n      }\n\n      dispatch({\n        type: SET_USER,\n        payload: {\n          uid: userSnapshot.id,\n          ...userSnapshot.data()\n        }\n      })\n    }).catch(err => console.log(err))\n  }\n\n  const signOut = async () => {\n    await auth.signOut();\n    dispatch({\n      type: SIGNED_OUT\n    })\n  }\n\n  const value = {\n    signInWithProvider,\n    signOut,\n    ...state,\n  }\n\n  return (\n    <AuthContext.Provider value={value}>\n      {children}\n    </AuthContext.Provider>\n  )\n}\n\nexport default AuthProvider;\n",["229","230"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/reducers/auth.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Router/ProtectedRoute.js",["231","232"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/loaders/Spinner/Spinner.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Loading/Loading.js",[],"/Users/alonzuman/code/alonzuma