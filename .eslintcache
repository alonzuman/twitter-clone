ight(window.innerHeight);\n  }\n\n  useEffect(() => {\n    window.addEventListener('resize', handleResize);\n    handleResize();\n\n    return () => window.removeEventListener('resize', handleResize)\n  }, [window])\n\n  return { width, height }\n}\n\nexport default useWindowSize;\n","/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/AddIcon.js",[],["317","318"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Feed/Feed.js",["319","320"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/lists/TweetsList/TweetsList.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/cards/TweetCard/TweetCardFooter.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/contexts/TweetsContext.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Tweet/Tweet.js",["321"],"import React, { useContext, useEffect, useState } from 'react';\nimport { ProfileContext } from '../../../contexts/ProfileContext';\nimport { TweetsContext } from '../../../contexts/TweetsContext';\nimport Avatar from '../../avatars/Avatar/Avatar';\nimport './Tweet.css';\nimport TweetFooter from './TweetFooter';\nimport TweetInput from './TweetInput';\n\nconst Tweet = ({ autoFocus, rows }) => {\n  const { isAuth, user: { displayName, username, avatar, uid }, isFetched } = useContext(ProfileContext);\n  const { addTweet } = useContext(TweetsContext);\n  const [tweet, setTweet] = useState({\n    displayName: '',\n    username: '',\n    content: '',\n    createdAt: Date.now(),\n    avatar: '',\n    likes: [],\n    replies: 0,\n    repliedTo: '',\n    uid: ''\n  })\n\n  useEffect(() => {\n    if (isAuth) {\n      setTweet({\n        ...tweet,\n        displayName,\n        username,\n        avatar,\n        uid,\n        createdAt: Date.now()\n      })\n    }\n  }, [isFetched])\n\n  const handleContentChange = e => {\n    setTweet({\n      ...tweet,\n      cont