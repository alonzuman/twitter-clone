export default Sidebar\n","/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/reportWebVitals.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/reducers/users.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/contexts/MessagesContext.js",["320","321"],"import React, { createContext, useReducer } from 'react'\nimport { db } from '../firebase';\nimport messagesReducer, { SET_ALL, IS_FETCHING, SET_MESSAGES, IS_FETCHING_MESSAGES, ERROR, SET_ONE } from '../reducers/messages';\nconst Chats = db.collection('chats');\n\nexport const initialState = {\n  isFetching: false,\n  isFetched: false,\n  isFetchingMessages: false,\n  isFetchedMessages: false,\n  chats: {},\n}\n\nexport const MessagesContext = createContext({});\n\nconst MessagesProvider = ({ children }) => {\n  const [state, dispatch] = useReducer(messagesReducer);\n\n  const getChats = (uid) => {\n    dispatch({\n      type: IS_FETCHING\n    })\n\n    Chats.where('participants', 'array-contains', uid).orderBy('updatedAt', 'desc').onSnapshot(snapshot => {\n      let data = {}\n      snapshot.docs.forEach(doc => {\n        return data = {\n          ...data,\n          [doc.id]: { ...doc.data() }\n        }\n      })\n      dispatch({\n        type: SET_ALL,\n        payload: data\n      })\n    })\n  }\n\n  const getChatMessages = (chatId) => {\n    dispatch({\n      type: IS_FETCHING_MESSAGES\n    })\n\n    Chats.doc(chatId).get().then(snapshot => {\n      if (snapshot.exists) {\n        Chats.doc(chatId).collection('messages').orderBy('createdAt', 'asc').onSnapshot(snapshot => {\n          const messages = snapshot.docs.map(doc => { return { id: doc.id, ...doc.data() } })\n          dispatch({\n            type: SET_MESSAGES,\n            payload: {\n              id: chatId,\n              messages\n            }\n          })\n        })\n      }\n    })\n  }\n\n  const startChat = (sender, receiver) => {\n    Chats.where('participants', 'array-contains', [sender.uid, receiver.uid]).get().then(snapshot => {\n      if (!snapshot.exists) {\n        const chatId = `${sender.uid}-${receiver.uid}`;\n        Chats.doc(chatId).set({\n          createdAt: Date.now(),\n          participants: [sender.uid, receiver.uid],\n          participantsData: [sender, receiver],\n          updatedAt: Date.now()\n        })\n      }\n    }).catch(err => console.log(err))\n  }\n\n  const sendMessage = (sender, chatId, content) => {\n    Chats.doc(chatId).set({\n      updatedAt: Date.now()\n    }, { merge: true });\n    Chats.doc(chatId).collection('messages').add({\n      createdAt: Date.now(),\n      content,\n      sender,\n      read: []\n    }).catch(err => console.log(err))\n  }\n\n  const value = {\n    getChats,\n    getChatMessages,\n    sendMessage,\n    startChat,\n    ...state\n  }\n\n  return (\n    <MessagesContext.Provider value={value}>\n      {children}\n    </MessagesContext.Provider>\n  )\n}\n\nexport default MessagesProvider\n","/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Footer/Footer.js",["322","323"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/buttons/PrimaryButton/PrimaryButton.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/AddIcon.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/HomeIcon.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/ProfileIcon.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Navbar/NavbarItem.js",["324"],"import React from 'react';\nimport { Link, NavLink } from 'react-router-dom';\nimport './NavbarItem.css';\n\nconst NavbarItem = ({ label, icon, link, onClick }) => {\n  if (link) {\n    return (\n      <NavLink exact activeClassName='navbarItem__container--active' onClick={onClick} to={link} className=\"baseFocus navbarItem__container\">\n        <span className=\"navbarItem__icon\">{icon}</span>\n        <span className=\"navbarItem__label\">{label}</span>\n      </NavLink>\n    )\n  } else {\n    return (\n      <li className=\"baseFocus navbarItem__container\" onClick={onClick}>\n        <span className=\"navbarItem__icon\">{icon}</span>\n        <span className=\"navbarItem__label\">{label}</span>\n      </li>\n    )\n  }\n}\n\nexport default NavbarItem\n","/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Messages/DesktopMessages.js",["325"],"import React, { useContext, useEffect, useState } from 'react'\nimport { useHistory } from 'react-router-dom';\nimport { MessagesContext } from '../../../contexts/MessagesContext';\nimport Chat from './Chat';\nimport ChatsList from './ChatsList';\nimport EmptyState from './EmptyState';\nimport './DesktopMessages.css';\n\nconst DesktopMessages = () => {\n  const { chats } = useContext(MessagesContext)\n  const history = useHistory();\n  const [activeChatId, setActiveChatId] = useState(history.location.pathname.split('/')[2]);\n  const noChats = Object.keys(chats || {}).length === 0;\n\n  useEffect(() => {\n    if (!noChats && !activeChatId) {\n      const firstChatId = Object.keys(chats)[0];\n      setActiveChatId(firstChatId);\n      history.push(`/messages/${firstChatId}`);\n    }\n  }, [chats])\n\n  return (\n    <div className='messages'>\n      {noChats && <EmptyState title=\"No messages\" body=\"Go send someone a message yo\" />}\n      {!noChats && <ChatsList />}\n      <div className='messages__desktopContainer'>\n        {!noChats && activeChatId && <Chat />}\n      </div>\n    </div>\n  )\n}\n\nexport default DesktopMessages\n","/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Reply/ReplyDialog.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/buttons/IconButton/IconButton.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Profile/ProfileHero/ProfileHero.js",["326","327","328"],"import React, { useContext, useState } from 'react';\nimport { Redirect, useHistory } from 'react-router-dom';\nimport MessageIcon from '../../../../assets/icons/MessageIcon';\nimport { MessagesContext } from '../../../../contexts/MessagesContext';\nimport { ProfileContext } from '../../../../contexts/ProfileContext';\nimport { UsersContext } from '../../../../contexts/UsersContext';\nimport useProfile from '../../../../hooks/useProfile';\nimport { displayBigNums } from '../../../../utils/maths';\nimport IconButton from '../../../buttons/IconButton/IconButton';\nimport PrimaryButton from '../../../buttons/PrimaryButton/PrimaryButton';\nimport './ProfileHero.css';\n\nconst ProfileHero = ({\n  uid,\n  displayName,\n  hero = 'https://pbs.twimg.com/profile_banners/25073877/1604214583/1500x500',\n  avatar,\n  username,\n  bio = '',\n  following = [],\n  followers = [],\n  currentUserProfile\n}) => {\n  const [isHovering, setIsHovering] = useState(false);\n  const currentUser = useProfile();\n  const { followUser, unfollowUser } = useContext(UsersContext);\n  const { startChat } = useContext(MessagesContext);\n  const { push } = useHistory();\n  const isFollowing = followers?.includes(currentUser.uid)\n\n  const handleMessageClick = () => {\n    startChat(currentUser, currentUserProfile);\n    return push(`/messages/${currentUser.uid}-${uid}`);\n  };\n  const handleFollowClick = () => isFollowing ? unfollowUser(currentUser.uid, uid) : followUser(currentUser.uid, uid);\n\n  return (\n    <div className='profileHero__container'>\n      <img src={hero} className='profileHero__banner' />\n      <div className='profileHero__body'>\n        <section className='profileHero__top'>\n          {avatar}\n          <div className='profileHero__controls'>\n            {currentUser.uid !== uid && (\n              <>\n                <IconButton onClick={handleMessageClick} variant='outlined' className='profileHero__controlsIcon'>\n                  <MessageIcon />\n                </IconButton>\n                <PrimaryButton onMouseEnter={() => setIsHovering(true)} onMouseLeave={() => setIsHovering(false)} variant={isFollowing ? '' : 'outlined'} onClick={handleFollowClick}>\n                  {!isFollowing ? 'Follow' : (isHovering ? 'Unfollow' : 'Following')}\n                </PrimaryButton>\n              </>)}\n          </div>\n        </section>\n        <section className='profileHero__text'>\n          <h1 className='profileHero__displayName'>{displayName}</h1>\n          <h3 className='profileHero__username'>{username}</h3>\n        </section>\n        <section className='profileHero__bioContainer'>\n          <p className='profileHero__bio'>{bio}</p>\n        </section>\n        <section className='profileHero__followsContainer'>\n          <span className='profileHero__followsCounter'>{displayBigNums(following.length)}</span>\n          <span className='profileHero__followsLabel'>Following</span>\n          <span className='profileHero__followsCounter'>{displayBigNums(followers.length)}</span>\n          <span className='profileHero__followsLabel'>Followers</span>\n        </section>\n      </div>\n    </div>\n  )\n}\n\nexport default ProfileHero\n","/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Messages/Chat.js",["329","330","331","332"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/cards/TweetCard/TweetCardFooter.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Messages/EmptyState.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Messages/MobileMessages.js",["333"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/index.js",[],["334","335"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/reducers/profile.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Navbar/Navbar.js",["336","337","338"],"/Users/alonzuman/co