e) => {\n    localStorage.setItem('fontSize', e.target.value);\n    setFontSize(e.target.value);\n  }\n\n  useEffect(() => {\n    const currentColor = localStorage.getItem('color');\n    const colors = ['blue', 'orange', 'yellow', 'purple', 'green', 'red']\n\n    if (!currentColor) {\n      localStorage.setItem('color', 'blue');\n      document.body.classList.add('color--blue');\n      setColor('blue')\n    } else {\n      colors.forEach(v => document.body.classList.remove(`color--${v}`))\n      document.body.classList.add(`color--${currentColor}`);\n      setColor(currentColor);\n    }\n  }, [localStorage.getItem('color')])\n\n  const handleColorChange = (color) => {\n    localStorage.setItem('color', color)\n    setColor(color)\n  }\n\n  const openDialog = () => setIsMoreOpen(true)\n  const closeDialog = () => setIsMoreOpen(false)\n\n  const value = {\n    isMoreOpen,\n    fontSize,\n    handleFontChange,\n    color,\n    handleColorChange,\n    background,\n    handleBackgroundChange,\n    openDialog,\n    closeDialog,\n    setBackground\n  }\n\n  return (\n    <ThemeContext.Provider value={value}>\n      {children}\n    </ThemeContext.Provider>\n  )\n}\n\nexport default ThemeProvider\n",["368","369"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/cards/TweetCard/TweetCardHeader.js",[],["370","371"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Tweet/TweetInput.js",["372"],"import React, { useEffect, useRef } from 'react';\nimport './TweetInput.css';\n\nconst TweetInput = ({ autoFocus, rows = 2, value, onChange }) => {\n  const inputRef = useRef(null);\n\n  useEffect(() => {\n    if (autoFocus) {\n      inputRef.current.focus();\n    }\n  }, [inputRef])\n\n  return (\n    <div className=\"tweetInput__container\">\n      <textarea\n        placeholder={`What's happening?`}\n        ref={inputRef}\n        className=\"tweetInput__input\"\n        rows={rows}\n        value={value}\n        onChange={onChange}\n      />\n    </div>\n  )\n}\n\nexport default TweetInput\n",["373","374"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/CommentIcon.js",[],["375","376"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Router/Router.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Home/Home.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/menus/MoreDialog.js",[],["377","378"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/menus/More.js",["379"],"import React, { useContext, useState } from 'react';\nimport { ThemeContext } from '../../contexts/ThemeContext';\nimport Slider from '../inputs/Slider/Slider';\nimport BackgroundButton from './BackgroundButton';\nimport ColorButton from './ColorButton';\nimport './More.css';\n\nconst More = () => {\n  const { handleBackgroundChange, handleFontChange, handleColorChange, background, fontSize, color: currentColor } = useContext(ThemeContext);\n\n  const backgrounds = [\n    { color: 'default', label: 'Default' },\n    { color: 'dim', label: 'Dim' },\n    { color: 'lights-out', label: 'Lights Out' },\n  ]\n\n  const colors = [\n    { color: 'blue' },\n    { color: 'yellow' },\n    { color: 'red' },\n    { color: 'purple' },\n    { color: 'orange' },\n    { color: 'green' },\n  ];\n\n  const fillValues = {\n    1: '0%',\n    2: '25%',\n    3: '50%',\n    4: '75%',\n    5: '100%'\n  }\n\n  return (\n    <div className='more'>\n      <p className='more__paragraph'>Manage your font size, color and background. These settings affect all the <br /> Twitter accounts on this browser.</p>\n      <section className='more__section'>\n        <h5 className='more__sectionTitle'>Font size</h5>\n        <div className='more__sectionContent'>\n          <Slider fillValue={fillValues[fontSize]} renderBefore='Aa' renderAfter='Aa' step={1} min={1} max={5} onChange={handleFontChange} value={fontSize} />\n        </div>\n      </section>\n      <section className='more__section'>\n        <h5 className='more__sectionTitle'>Color</h5>\n        <div className='more__sectionContent more__colorButtons'>\n          {colors.map(({ color }) => <ColorButton key={color} isSelected={currentColor === color} color={color} onClick={() => handleColorChange(color)} />)}\n        </div>\n      </section>\n      <section className='more__section'>\n        <h5 className='more__sectionTitle'>Background</h5>\n        <div className='more__sectionContent'>\n          <div className='more__bottomButtons'>\n            {backgrounds.map(({ color, label }) => (\n              <BackgroundButton\n                color={color}\n                isSelected={background === color}\n                onClick={() => handleBackgroundChange(color)}>\n                {label}\n              </BackgroundButton>\n            ))}\n          </div>\n        </div>\n      </section>\n    </div>\n  )\n}\n\nexport default More\n","/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Container/Container.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Router/ProtectedRoute.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Footer/FooterItem.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/buttons/SecondaryButton/SecondaryButton.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Reply/Reply.js",["380"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/CloseIcon.js",[],["381","382"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Tweet/TweetFooter.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/menus/ColorButton.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/CheckIcon.js",[],"/Users/alonzuman/code/al