s = 2, tweet, editTweet }) => {\n  const inputRef = useRef(null);\n\n  useEffect(() => {\n    if (autoFocus) {\n      inputRef.current.focus();\n    }\n  }, [inputRef])\n\n  return (\n    <div className=\"tweetInput__container\">\n      <textarea\n        placeholder={`What's happening?`}\n        ref={inputRef}\n        className=\"tweetInput__input\"\n        rows={rows}\n        value={tweet.content}\n        onChange={editTweet}\n      />\n    </div>\n  )\n}\n\nexport default TweetInput\n","/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Router/Router.js",[],["236","237"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Auth/Auth.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/contexts/AuthContext.js",["238","239"],"import firebase from 'firebase';\nimport React, { createContext, useEffect, useReducer } from \"react\";\nimport { auth, db } from '../firebase';\nimport authReducer, { IS_FETCHING } from \"../reducers/auth\";\nimport { IS_UPDATING, SIGNED_OUT, SET_USER } from '../reducers/auth';\nconst Users = db.collection('users');\n\nconst initialState = {\n  isFetching: false,\n  isFetched: false,\n  isUpdating: false,\n  isUpdated: false,\n  user: {}\n}\n\nexport const AuthContext = createContext({});\n\nconst AuthProvider = ({ children }) => {\n  const [state, dispatch] = useReducer(authReducer, initialState);\n\n  useEffect(() => {\n    auth.onAuthStateChanged(user => {\n      dispatch({\n        type: IS_FETCHING\n      })\n      if (user) {\n        setUser(user.uid)\n      } else {\n        dispatch({\n          type: SIGNED_OUT\n        })\n      }\n    })\n  }, [auth.currentUser])\n\n  const setUser = async (uid) => {\n    Users.doc(uid).get().then(snapshot => {\n      const user = {\n        id: snapshot.id,\n        ...snapshot.data()\n      };\n      dispatch({\n        type: SET_USER,\n        payload: user\n      })\n    })\n  }\n\n  const signInWithProvider = async (provider) => {\n    dispatch({\n      type: IS_FETCHING\n    })\n\n    const firebaseProvider = () => {\n      switch (provider) {\n        case 'facebook': return new firebase.auth.FacebookAuthProvider();\n        case 'google': return new firebase.auth.GoogleAuthProvider();\n        default: return null\n      }\n    }\n\n    await auth.setPersistence(firebase.auth.Auth.Persistence.LOCAL).then(async () => {\n      const { user: { uid, displayName, email, photoURL: avatar, phoneNumber, emailVerified } } = await auth.signInWithPopup(firebaseProvider());\n\n      const userSnapshot = await Users.doc(uid).get();\n      if (!userSnapshot.exists) {\n        await Users.doc(uid).set({ uid, displayName, email, photoURL: avatar, phoneNumber, emailVerified }, { merge: true })\n      }\n\n      dispatch({\n        type: SET_USER,\n        payload: {\n          uid: userSnapshot.id,\n          ...userSnapshot.data()\n        }\n      })\n    }).catch(err => console.log(err))\n  }\n\n  const signOut = async () => {\n    await auth.signOut();\n    dispatch({\n      type: SIGNED_OUT\n    })\n  }\n\n  const value = {\n    signInWithProvider,\n    signOut,\n    ...state,\n  }\n\n  return (\n    <AuthContext.Provider value={value}>\n      {children}\n    </AuthContext.Provider>\n  )\n}\n\nexport default AuthProvider;\n",["240","241"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/reducers/auth.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Router/ProtectedRoute.js",["242","243"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/loaders/Spinner/Spinner.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Loading/Loading.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/TwitterIcon.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/index.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src