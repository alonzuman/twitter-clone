ayout/Messages/ChatCard.js",["373","374","375"],"import React, { useContext, useEffect, useState } from 'react';\nimport { useHistory } from 'react-router-dom';\nimport './ChatCard.css';\nimport { db } from '../../../firebase';\nimport { MessagesContext } from '../../../contexts/MessagesContext';\nimport useProfile from '../../../hooks/useProfile';\nimport Avatar from '../../avatars/Avatar/Avatar';\nimport moment from 'moment';\n\nconst ChatCard = ({ chatId }) => {\n  const { chats } = useContext(MessagesContext);\n  const { uid } = useProfile();\n  const { push, location } = useHistory();\n  const currentChat = chats[chatId];\n  const currentUser = currentChat.participantsData.find(user => user.uid !== uid)\n  const { displayName, avatar, username } = currentUser;\n\n  const handleClick = () => {\n    const chatUrl = `/messages/${chatId}`;\n    if (location.pathname !== chatUrl) {\n      push(chatUrl);\n    }\n  }\n\n  return (\n    <div className='chatCard' onClick={handleClick}>\n      <div className='chatCard__avatar'>\n        <Avatar src={avatar} />\n      </div>\n      <div className='chatCard__main'>\n        <div className='chatCard__header'>\n          <h4 className='chatCard__displayName'>{displayName}</h4>\n          <p className='chatCard__username'>@{username}</p>\n        </div>\n        <p>{moment(currentChat?.updatedAt).fromNow()}</p>\n      </div>\n    </div>\n  )\n}\n\nexport default ChatCard\n","/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Auth/Auth.js",["376"],"import React, { useContext } from 'react';\nimport firebase from 'firebase';\nimport { Redirect } from 'react-router-dom';\nimport TwitterIcon from '../../../assets/icons/TwitterIcon';\nimport PrimaryButton from '../../buttons/PrimaryButton/PrimaryButton';\nimport Loading from '../SplashScreen/SplashScreen';\nimport './Auth.css';\nimport { auth } from '../../../firebase';\nimport { useAuthState } from 'react-firebase-hooks/auth';\n\nconst Auth = () => {\n  const [user, loading] = useAuthState(auth)\n\n  const signInWithGoogle = () => {\n    const provider = new firebase.auth.GoogleAuthProvider();\n    auth.signInWithPopup(provider);\n  }\n\n  const continueAsGuest = () => {\n    auth.signInAnonymously();\n  }\n\n  if (user) {\n    return <Redirect to='/' />\n  } else if (!loading) {\n    return (\n      <div className='auth'>\n        <div className='auth__hero'>\n          <TwitterIcon className='auth__icon' />\n          <div className='auth__text'>\n            <h1 className='auth__header'>See whatâ€™s happening in <br />the world right now</h1>\n          </div>\n          <PrimaryButton disabled={loading} isLoading={loading} className='auth__button' onClick={signInWithGoogle}>Sign in with Google</PrimaryButton>\n          <PrimaryButton onClick={continueAsGuest} className='auth__button' variant='outlined'>Continue as Guest</PrimaryButton>\n        </div>\n      </div>\n    )\n  } else {\n    return <Loading size='lg' />\n  }\n}\n\nexport default Auth\n","/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/TwitterIcon.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/HomeIcon.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/ProfileIcon.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Navbar/NavbarPopper.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Tweet/TweetDialog.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/ArrowLeftIcon.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/cards/TweetCard/TweetCardAvatar.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Tweet/TweetInput.js",["377"],"import React, { useEffect, useRef } from 'react';\nimport './TweetInput.css';\n\nconst TweetInput = ({ autoFocus, rows = 2, value, onChange }) => {\n