vider = new firebase.auth.GoogleAuthProvider();\n    auth.signInWithPopup(provider);\n  }\n\n  const continueAsGuest = () => {\n    auth.signInAnonymously();\n  }\n\n  if (user) {\n    return <Redirect to='/' />\n  } else if (!loading) {\n    return (\n      <div className='auth'>\n        <div className='auth__hero'>\n          <TwitterIcon className='auth__icon' />\n          <div className='auth__text'>\n            <h1 className='auth__header'>See what’s happening in <br />the world right now</h1>\n          </div>\n          <PrimaryButton disabled={loading} isLoading={loading} className='auth__button' onClick={signInWithGoogle}>Sign in with Google</PrimaryButton>\n          <PrimaryButton onClick={continueAsGuest} className='auth__button' variant='outlined'>Continue as Guest</PrimaryButton>\n        </div>\n      </div>\n    )\n  } else {\n    return <Loading size='lg' />\n  }\n}\n\nexport default Auth\n","/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/TwitterIcon.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/HomeIcon.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/ProfileIcon.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Navbar/NavbarPopper.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Tweet/TweetDialog.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/ArrowLeftIcon.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/cards/TweetCard/TweetCardAvatar.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Tweet/TweetInput.js",["373"],"import React, { useEffect, useRef } from 'react';\nimport './TweetInput.css';\n\nconst TweetInput = ({ autoFocus, rows = 2, value, onChange }) => {\n  const inputRef = useRef(null);\n\n  useEffect(() => {\n    if (autoFocus) {\n      inputRef.current