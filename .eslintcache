ClickAway;\n","/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/contexts/UsersContext.js",[],["388","389"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/cards/TweetCard/TweetCardBody.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/CommentIcon.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Sidebar/Sidebar.js",["390"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Feed/Feed.js",["391","392"],"import React, { useContext, useEffect } from 'react'\nimport { TweetsContext } from '../../../contexts/TweetsContext'\nimport './Feed.css';\nimport Tweet from '../Tweet/Tweet';\nimport TweetsList from '../../lists/TweetsList/TweetsList';\nimport Header from '../Header/Header';\n\nconst Feed = () => {\n  const { tweets, fetchTweets, isFetching, isFetched } = useContext(TweetsContext);\n\n  useEffect(() => {\n    fetchTweets({\n      queryParams: {\n        isTweet: true\n      },\n      key: 'all'\n    });\n  }, [])\n\n  return (\n    <div className='feed'>\n      <Header title='Home' />\n      <div className=\"feed__tweet\">\n        <Tweet />\n      </div>\n      <TweetsList isLoading={isFetching} tweets={tweets.all} />\n    </div>\n  )\n}\n\nexport default Feed\n","/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Home/Home.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Messages/ChatCard.js",["393","394","395"],"import React, { useContext, useEffect, useState } from 'react';\nimport { useHistory } from 'react-router-dom';\nimport './ChatCard.css';\nimport { db } from '../../../firebase';\nimport { MessagesContext } from '../../../contexts/MessagesContext';\nimport useProfile from '../../../hooks/useProfile';\nimport Avatar from '../../avatars/Avatar/Avatar';\nimport moment from 'moment';\n\nconst ChatCard = ({ chatId }) => {\n  const { chats } = useCont