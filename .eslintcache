],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Router/ProtectedRoute.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Footer/FooterItem.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/buttons/SecondaryButton/SecondaryButton.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Reply/Reply.js",["240"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/CloseIcon.js",[],["241","242"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Tweet/TweetFooter.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/menus/ColorButton.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/CheckIcon.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/reducers/messages.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/TweetPage/TweetPage.js",["243","244"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Sidebar/Sidebar.js",["245"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/reportWebVitals.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/reducers/users.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/contexts/MessagesContext.js",["246","247"],"import React, { createContext, useReducer } from 'react'\nimport { db } from '../firebase';\nimport messagesReducer, { SET_ALL, IS_FETCHING, SET_MESSAGES, IS_FETCHING_MESSAGES, ERROR, SET_ONE } from '../reducers/messages';\nconst Chats = db.collection('chats');\n\nexport const initialState = {\n  isFetching: false,\n  isFetched: false,\n  isFetchingMessages: false,\n  isFetchedMessages: false,\n  chats: {},\n}\n\nexport const MessagesContext = createContext({});\n\nconst MessagesProvider = ({ children }) => {\n  const [state, dispatch] = useReducer(messagesReducer);\n\n  const getChats = (uid) => {\n    dispatch({\n      type: IS_FETCHING\n    })\n\n    Chats.where('participants', 'array-contains', uid).orderBy('updatedAt', 'desc').onSnapshot(snapshot => {\n      let data = {}\n      snapshot.docs.forEach(doc => {\n        return data = {\n          ...data,\n          [doc.id]: { ...doc.data() }\n        }\n      })\n      dispatch({\n        type: SET_ALL,\n        payload: data\n      })\n    })\n  }\n\n  const getChatMessages = (chatId) => {\n    dispatch({\n      type: IS_FETCHING_MESSAGES\n    })\n\n    Chats.doc(chatId).get().then(snapshot => {\n      if (snapshot.exists) {\n        Chats.doc(chatId).collection('messages').orderBy('createdAt', 'asc').onSnapshot(snapshot => {\n          const messages = snapshot.docs.map(doc => { return { id: doc.id, ...doc.data() } })\n          dispatch({\n            type: SET_MESSAGES,\n            payload: {\n              id: chatId,\n              messages\n            }\n          })\n        })\n      }\n    })\n  }\n\n  const startChat = (sender, receiver) => {\n    Chats.where('participants', 'array-contains', [sender.uid, receiver.uid]).get().then(snapshot => {\n      if (!snapshot.exists) {\n        const chatId = `${sender.uid}-${receiver.uid}`;\n        Chats.doc(chatId).set({\n          createdAt: Date.now(),\n          participants: [sender.uid, receiver.uid],\n          participantsData: [sender, receiver],\n          updatedAt: Date.now()\n        })\n      }\n    }).catch(err => console.log(err))\n  }\n\n  const sendMessage = (sender, chatId, content) => {\n    Chats.doc(chatId).set({\n      updatedAt: Date.now()\n    }, { merge: true });\n    Chats.doc(chatId).collection('messages').add({\n      createdAt: Date.now(),\n      content,\n      sender,\n      read: []\n    }).catch(err => console.log(err))\n  }\n\n  const value = {\n    getChats,\n    getChatMessages,\n    sendMessage,\n    startChat,\n    ...state\n  }\n\n  return (\n    <MessagesContext.Provider value={value}>\n      {children}\n    </MessagesContext.Provider>\n  )\n}\n\nexport default MessagesProvider\n","/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Footer/Footer.js",["248","249"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/buttons/PrimaryButton/PrimaryButton.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/AddIcon.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/HomeIcon.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/ProfileIcon.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Navbar/NavbarItem.js",["250"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Messages/DesktopMessages.js",["251"],"import React, { useContext, useEffect, useState } from 'react'\nimport { useHistory } from 'react-router-dom';\nimport { MessagesContext } from '../../../contexts/MessagesContext';\nimport Chat from './Chat';\nimport ChatsList from './ChatsList';\nimport EmptyState from './EmptyState';\n\nconst DesktopMessages = () => {\n  const { chats } = useContext(MessagesContext)\n  const history = useHistory();\n  const [activeChatId, setActiveChatId] = useState(history.location.pathname.split('/')[2]);\n  const noChats = Object.keys(chats || {}).length === 0;\n\n  useEffect(() => {\n    if (!noChats && !activeChatId) {\n      const firstChatId = Object.keys(chats)[0];\n      setActiveChatId(firstChatId);\n      history.push(`/messages/${firstChatId}`);\n    }\n  }, [chats])\n\n  return (\n    <div className='messages'>\n      {noChats && <EmptyState title=\"No messages\" body=\"Go send someone a message yo\" />}\n      {!noChats && <ChatsList />}\n      {!noChats && activeChatId && <Chat />}\n    </div>\n  )\n}\n\nexport default DesktopMessages\n","/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Reply/ReplyDialog.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/buttons/IconButton/IconButton.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Profile/ProfileHero/ProfileHero.js",["252","253","254"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Messages/Chat.js",["255","256"],"import React, { useContext, useEffect, useRef, useState } from 'react';\nimport { useHistory } from 'react-router-dom';\nimport ArrowLeftIcon from '../../../assets/icons/ArrowLeftIcon';\nimport SendIcon from '../../../assets/icons/SendIcon';\nimport { MessagesContext } from '../../../contexts/MessagesContext';\nimport useProfile from '../../../hooks/useProfile';\nimport Avatar from '../../avatars/Avatar/Avatar';\nimport IconButton from '../../buttons/IconButton/IconButton';\nimport Spinner from '../../loaders/Spinner/Spinner';\nimport Header from '../Header/Header';\nimport './Chat.css';\nimport ChatMessage from './ChatMessage';\n\nconst Chat = ({ onBack }) => {\n  const { chats, getChatMessages, sendMessage, isFetchingMessages, isFetchedMessages } = useContext(MessagesContext);\n  const [newMessage, setNewMessage] = useState('');\n  const history = useHistory()\n  const { uid } = useProfile();\n  const bottomRef = useRef(null);\n  const chatId = history.location.pathname.split('/')[2];\n  const currentChat = chats[chatId];\n  const messages = currentChat?.messages;\n  const currentChatUser = currentChat?.participantsData?.find(user => user.uid !== uid)\n\n  const scrollIntoView = () => {\n    if (bottomRef && messages) {\n      bottomRef?.current?.scrollIntoView({ behavior: 'smooth' });\n    }\n  }\n\n  useEffect(() => {\n    if (chatId) {\n      getChatMessages(chatId);\n    }\n  }, [history.location.pathname])\n\n  useEffect(() => {\n    scrollIntoView();\n  }, [messages])\n\n  const handleSubmit = async e => {\n    e.preventDefault();\n    await sendMessage(uid, chatId, newMessage)\n    getChatMessages(chatId);\n    setNewMessage('')\n    scrollIntoView();\n  }\n\n  const headerAction = (\n    <div className='chat__headerActions'>\n      {onBack && <IconButton className='chat__headerBackButton' onClick={onBack}><ArrowLeftIcon /></IconButton>}\n      <Avatar className='chat__headerAvatar' size='xs' src={currentChatUser?.avatar} />\n    </div>\n  )\n\n  return (\n    <div className='chat'>\n      <Header title={currentChatUser?.displayName} action={headerAction} size='xs' />\n      {isFetchingMessages && !messages && <Spinner className='chat__spinner' size='md' />}\n      {isFetchedMessages &&\n      <ul id='chatMessages' className='chat__messages'>\n        {messages?.map(({ content, sender, createdAt, read, id }) => <ChatMessage key={id} id={id} content={content} sender={sender} createdAt={createdAt} read={read} />)}\n        <div ref={bottomRef} />\n      </ul>}\n      <div className='chat__footer'>\n        <form className='chat__inputForm' onSubmit={handleSubmit}>\n          <input className='chat__inputField' value={newMessage} onChange={e => setNewMessage(e.target.value)} placeholder='type in yo msg' />\n          <IconButton disabled={isFetchingMessages} className='chat__sendButton' type='submit'>\n            <SendIcon className='chat__sendIcon' />\n          </IconButton>\n        </form>\n      </div>\n    </div>\n  )\n}\n\nexport default Chat\n","/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/cards/TweetCard/TweetCardFooter.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Messages/EmptyState.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Messages/MobileMessages.js",["257"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/index.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/reducers/profile.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components