../../hooks/useWindowSize';\nimport './Sidebar.css'\n\nconst Sidebar = () => {\n  const { width } = useWindowSize();\n\n  if (width > 768) {\n    return (\n      <div role=\"side-bar\" className='sidebar'>\n        <div />\n      </div>\n    )\n  } else {\n    return null;\n  }\n}\n\nexport default Sidebar\n","/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/reportWebVitals.js",[],["357","358"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/reducers/users.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/contexts/MessagesContext.js",["359","360"],"import React, { createContext, useReducer } from 'react'\nimport { db } from '../firebase';\nimport messagesReducer, { SET_ALL, IS_FETCHING, SET_MESSAGES, IS_FETCHING_MESSAGES, ERROR, SET_ONE } from '../reducers/messages';\nconst Chats = db.collection('chats');\n\nexport const initialState = {\n  isFetching: false,\n  isFetched: false,\n  isFetchingMessages: false,\n  isFetchedMessages: false,\n  chats: {},\n}\n\nexport const MessagesContext = createContext({});\n\nconst MessagesProvider = ({ children }) => {\n  const [state, dispatch] = useReducer(messagesReducer);\n\n  const getChats = (uid) => {\n    dispatch({\n      type: IS_FETCHING\n    })\n\n    Chats.where('participants', 'array-contains', uid).orderBy('updatedAt', 'desc').onSnapshot(snapshot => {\n      let data = {}\n      snapshot.docs.forEach(doc => {\n        return data = {\n          ...data,\n          [doc.id]: { ...doc.data() }\n        }\n      })\n      dispatch({\n        type: SET_ALL,\n        payload: data\n      })\n    })\n  }\n\n  const getChatMessages = (chatId) => {\n    dispatch({\n      type: IS_FETCHING_MESSAGES\n    })\n\n    Chats.doc(chatId).get().then(snapshot => {\n      if (snapshot.exists) {\n        Chats.doc(chatId).collection('messages').orderBy('createdAt', 'asc').onSnapshot(snapshot => {\n          const messages = snapshot.docs.map(doc => { return { id: doc.id, ...doc.data() } })\n          dispatch({\n            type: SET_MESSAGES,\n            payload: {\n              id: chatId,\n              messages\n            }\n          })\n        })\n      }\n    })\n  }\n\n  const startChat = (sender, receiver) => {\n    Chats.where('participants', 'array-contains', [sender.uid, receiver.uid]).get().then(snapshot => {\n      if (!snapshot.exists) {\n        const chatId = `${sender.uid}-${receiver.uid}`;\n        Chats.doc(chatId).set({\n          createdAt: Date.now(),\n          participants: [sender.uid, receiver.uid],\n          participantsData: [sender, receiver],\n          updatedAt: Date.now()\n        })\n      }\n    }).catch(err => console.log(err))\n  }\n\n  const sendMessage = (sender, chatId, content) => {\n    Chats.doc(chatId).set({\n      updatedAt: Date.now()\n    }, { merge: true });\n    Chats.doc(chatId).collection('messages').add({\n      createdAt: Date.now(),\n      content,\n      sender,\n      read: []\n    }).catch(err => console.log(err))\n  }\n\n  const value = {\n    getChats,\n    getChatMessages,\n    sendMessage,\n    startChat,\n    ...state\n  }\n\n  return (\n    <MessagesContext.Provider value={value}>\n      {children}\n    </MessagesContext.Provider>\n  )\n}\n\nexport default MessagesProvider\n","/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Footer/Footer.js",["361","362"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/buttons/PrimaryButton/PrimaryButton.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/AddIcon.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/HomeIcon.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/ProfileIcon.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Navbar/NavbarItem.js",["363"],"import React from 'react';\nimport { Link, NavLink } from 'react-router-dom';\nimport './N