Size = localStorage.getItem('fontSize');\n\n    if (!currentFontSize) {\n      localStorage.setItem('fontSize', '3')\n      document.body.classList.add(`font-${currentFontSize}`);\n      setFontSize('3');\n    } else {\n      for (let i = 0; i < 5; i++) {\n        document.body.classList.remove(`font-${i + 1}`);\n      }\n      document.body.classList.add(`font-${currentFontSize}`);\n      setFontSize(currentFontSize);\n    }\n  }, [fontSize])\n\n  const handleFontChange = (e) => {\n    localStorage.setItem('fontSize', e.target.value);\n    setFontSize(e.target.value);\n  }\n\n  const handleColorChange = (color) => {\n  }\n\n  const openDialog = () => setIsMoreOpen(true)\n  const closeDialog = () => setIsMoreOpen(false)\n\n  const value = {\n    isMoreOpen,\n    fontSize,\n    handleFontChange,\n    color,\n    handleColorChange,\n    background,\n    handleBackgroundChange,\n    openDialog,\n    closeDialog,\n    setBackground\n  }\n\n  return (\n    <ThemeContext.Provider value={value}>\n      {children}\n    </ThemeContext.Provider>\n  )\n}\n\nexport default ThemeProvider\n",["263","264"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/cards/TweetCard/TweetCardHeader.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/assets/icons/HeartIcon.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Profile/ProfileHero/ProfileHero.js",["265","266","267","268"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/hooks/useProfile.js",["269"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/utils/maths.js",[],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/layout/Profile/Profile.js",["270","271","272","273"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/components/cards/TweetCard/TweetCard.js",[],["274","275"],"/Users/alonzuman/code/alonzuman/projects/fun/twitter-clone/src/contexts/ProfileContext.js",["276","277"],"/Users